<%= form_for(@task) do |f| %>
	<%= f.label :name %>	
	<%= f.text_field :name %>

	<%= f.label :description %>	
	<%= f.text_field :description %>

	<%= f.label :real_effort %>	
	<%= f.text_field :real_effort %>

	<%= f.label :estimative_effort %>	
	<%= f.text_field :estimative_effort %>

	<%= f.submit %>
<% end %>